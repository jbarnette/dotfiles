[alias]
        cleanup = !git branch --merged | grep -v '*' | xargs git branch -d

[github]
	user = jbarnette

[hub]
	protocol = https

[push]
	default = simple

[user]
  name = John Barnette
  email = jbarnette@github.com

[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
